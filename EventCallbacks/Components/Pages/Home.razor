@page "/"
@rendermode InteractiveServer

<PageTitle>Home</PageTitle>

<h1>Hello, world!</h1>

<Search OnSearch="OnSearch" />
<div>You Searched for: @SearchText</div>

@code {
    public string SearchText { get; set; } = string.Empty;

    public void OnSearch(string text)
    {
        SearchText = text;
    }
}
